{
  "name": "Bookmarks书签管理-疯子的天空",
  "version": "2.0",
  "manifest_version": 3,
  "description": "支持横向显示更多书签(收藏夹)，让书签再次变得好用！make bookmarks great again!",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "bookmark.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "favicon",
    "fontSettings",
    "storage"
  ]
}